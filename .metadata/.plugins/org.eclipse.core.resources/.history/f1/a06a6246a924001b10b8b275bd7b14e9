
public class FibanocciReverse {

	public static void main(String[] args) {

		//int n=10;
		ReverseFibanocci(10);
	}

 static void ReverseFibanocci(int n){
		int a[] =new int[n];
		a[0] =0;
		a[1]=1;
		for(int i=2;i<=n;i++){
			a[i]=a[i-2]+a[i-1];
			System.out.println(a[i]);
		}
		for(int i=n-1;i>=0;i--){
			System.out.println(a[i]);
		}
		
	}
}	
