
public class Factorial {

	public static void main(String[] args) {
		// n*(n-1)
		int output = 0;
		int input =3;
		int length = input;
		int outputval = input;

		for (int i = 1; i < length; i++) {
			output = (input - 1);
			outputval = outputval * output;
			outputval=output;
			input = output;
			System.out.println(outputval);
		}
		System.out.println("output : " + outputval);
	}

}

/*
 * 3!
 * 
 * 3*(3-1)*(3-2)
 */