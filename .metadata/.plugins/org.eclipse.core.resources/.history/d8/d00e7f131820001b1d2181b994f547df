package collections;

import java.util.Arrays;
import java.util.List;

public class reverse_assign {

	public static void main(String[] args){
		List<Integer> num= Arrays.asList(2,3,3,4,4,5,6,1);
		

	int j=num.get(0);
	//System.out.println(j);
		for(int i=num.size()-1; i>0; i--){
			
			if (j<num.get(i)){
				j=num.get(i);
				
			}
			System.out.println(j);
		}
		
		
	}
}
