
public class Factorial {

	public static void main(String[] args) {
		//n*(n-1)
		int output =0;
		int input = 10;
		int length = input;
		int outputval = 1;
	
		for (int i=1; i<length; i++){
			output = (input-1);
			outputval  =outputval * output;
			input=output;
			System.out.println(outputval);
		}
		//outputval= outputval * output;
	}

}

/*
3!

3*(3-1)*(3-2)
*/