
public class Palindrome {

	public static void main(String[] args) {

		String input = "DINESH";	
		char output = 0 ;
		char ch[] = input.toCharArray();
		for(int i=0;i<input.length();i++){
			//System.out.println(ch[i]);
		}		
		for(int j=input.length()-1;j>=0;j--){
			
			output =ch[j];
			System.out.println(output);
		}
	//	System.out.println(output);
	}
}
