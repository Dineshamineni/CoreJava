package collections;

public class Hello {

	public static void main(String[] args) {
		String input = "ahceclwlxo";
		String output = "";
		//String output = input.charAt(index)
		for(int i=1; i<input.length();i=i+2){
			output =output + input.charAt(i);
			//System.out.println(output);
			}
		System.out.println(output);
		}

	}


